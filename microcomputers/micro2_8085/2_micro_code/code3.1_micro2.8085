MVI E,09H
START:
	MVI A,00H
	CMA
	STA 3000H
INPUT:
	LDA 2000H
	CPI 00H
	JZ START
	MVI C,00H
	JMP LOOP1

LOOP1:
	RRC
	JC HELP 
	INR C 
	CMP E
	JNZ LOOP1 
	JMP INPUT

HELP:
	MVI A,01H
	
LOOP2:
	RLC
	DCR C
	JZ PRINT
	JMP LOOP2
 
PRINT: 
